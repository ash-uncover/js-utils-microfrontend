{
  "name": "@uncover/js-utils-microfrontend",
  "version": "1.0.0",
  "author": "aSH <ash.uncover@gmail.com>",
  "description": "Util project for micro frontends management",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ash-uncover/js-utils-microfrontend"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "bundles/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run build:tsc & npm run build:bundle",
    "build:bundle": "webpack --config webpack.config.build.js --mode production",
    "build:docs": "npm i && webpack --config webpack.config.docs.js --mode production",
    "build:tsc": "tsc",
    "start": "concurrently \"npm:start:*\"",
    "start:main": "npm i && webpack serve --progress --mode development --config webpack.config.docs.main.js",
    "start:child": "npm i && webpack serve --progress --mode development --config webpack.config.docs.child.js",
    "test": "jest --env=jsdom",
    "test:coverage": "jest --env=jsdom --collectCoverage"
  },
  "dependencies": {
    "@uncover/js-utils": "^0.0.11",
    "@uncover/js-utils-logger": "^0.1.5"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.11",
    "@types/react-dom": "^18.0.9",
    "babel-loader": "^9.1.0",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.2",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
